enum FuelType {
    PETROL91
    PETROL95
    PETROL100
    DIESEL
    HYBRID
    ELECTRIC
}

type CarDetail {
    licensePlate: ID
    engineSize: Float
    horsepower: Int
    torque: Int
    fuelType: FuelType
    fuelEcoUrban: Float
    fuelEcoRural: Float
    make: String
    model: String
    year: Int
    country: String
    fuelPrice: FuelPrice
}

type FuelPrice {
    country: ID
    petrol91: Float
    petrol95: Float
    petrol100: Float
    diesel: Float
    ruc: Float
    electric: Float
}

type Person {
    email: ID
    firstName: String
    middleName: String
    lastName: String
    age: Int
    birthday: String
    personCars: [PersonCar]
}

type PersonCar {
    email: ID
    licensePlate: ID
    urbanRuralRatio: Float
    fuelCost100km: Float
}

type Query {
    carDetails: [CarDetail]
    carByPlate(licensePlate: ID): CarDetail
    fuelPrices: [FuelPrice]
    people: [Person]
    personByID(email: ID): Person
    peopleByFirstName(firstName: String): [Person]
    peopleByFirstLastName(firstName: String, lastName: String): [Person]
    personCars: [PersonCar]
    personCarByID(email: ID): [PersonCar]
    personCarByPlate(licensePlate: ID): [PersonCar]
    personCarByIDPlate(email: ID, licensePlate: ID): PersonCar
}

